        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// movimiento personaje


// entradas personaje
key_right = keyboard_check (vk_right);
key_left = -keyboard_check (vk_left);
key_jump = keyboard_check_pressed (vk_up);
key_down = -keyboard_check (vk_down);

// reaccion a entradas
move = key_left + key_right;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;

if (place_meeting (x, y+1, obj_bolque))
   {
   if (key_jump) vsp = -jumpspeed;
   }

var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// colision horizontal
if (place_meeting (x+hsp_final, y, obj_bolque))
  {
       while (!place_meeting (x+sign(hsp_final), y, obj_bolque))
       {
       x += sign (hsp_final);
       }
       hsp_final = 0;
       hsp = 0;
  }
x += hsp_final;

// colision vertical
if (place_meeting (x, y+vsp, obj_bolque))
  {
       while (!place_meeting (x, y+sign(vsp), obj_bolque))
       {
       y += sign (vsp);
       }
       vsp = 0;
  }
y += vsp;

//animacion

if (move!=0) image_xscale = move;
if (place_meeting (x,y+1,obj_bolque))
{
      if (move!=0) sprite_index = spr_personaje; else sprite_index = spr_PD;
}
else
{
  if (vsp&lt;0) sprite_index = spr_personaje; else sprite_index = spr_PI;
  }
</string>
